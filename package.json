{
  "name": "dextoolsbot",
  "version": "0.1.0",
  "description": "A DEXTOOLS bot",
  "scripts": {
    "build": "esbuild bot.ts --platform=node --outfile=build/bot.js",
    "bundle": "esbuild bot.ts --bundle --platform=node --outfile=bundle/bot_bundle.js",
    "start": "node build/bot.js",
    "buildstart": "esbuild bot.ts --platform=node --outfile=build/bot.js && node build/bot.js"
  },
  "type": "module",
  "author": "neolectron, satoshiba",
  "dependencies": {
    "dotenv": "^10.0.0",
    "esbuild": "^0.13.6",
    "puppeteer": "^10.2.0",
    "puppeteer-extra": "^3.1.18",
    "puppeteer-extra-plugin-stealth": "^2.7.8"
  }
}
